{
	"name": "aimeos/aimeos-typo3-dist",
	"description": "Aimeos TYPO3 distribution for web shops, portals and market places",
	"keywords": ["e-commerce", "shop", "portal", "market place"],
	"homepage": "https://www.aimeos.org",
	"type": "typo3-cms-extension",
	"license": "LGPL-3.0",
	"support": {
		"source": "https://github.com/aimeos/aimeos-typo3-dist",
		"issues": "https://github.com/aimeos/aimeos-typo3-dist/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": ">=5.4.0",
		"dmitryd/typo3-realurl": "~2.2",
		"bk2k/bootstrap-package": "~8.0",
		"sjbr/static-info-tables": "~6.0",
		"aimeos/aimeos-typo3": "~2017.10"
	},
	"replace": {
		"aimeos_dist": "self.version"
	},
	"autoload": {
		"classmap": ["Classes"],
		"psr-4": {
			"Aimeos\\Aimeos\\": "Classes"
		}
	}
}
